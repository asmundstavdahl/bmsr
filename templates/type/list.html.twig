{% extends 'base.html.twig' %}

{% block body %}
	<h1>Oversikt over typer ting</h1>
	<table>
		<tr>
			<th>Navn</th>
			<th>Antall</th>
		</tr>
		{% for type in types %}
		<tr>
			<td>
				<a href="{{ path("type_show", {"type_id": type.id}) }}">
					{{ type.name }}
				</a>
			</td>
			<td>{{ type.count }}</td>
		</tr>
		{% endfor %}
	</table>
{% endblock %}
